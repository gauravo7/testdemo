{
    "name": "trumath",
    "version": "2.1.0",
    "description": "Trumath - Interactive mathematics classes for 8th, 9th and 10th classes",
    "main": "index.js",
    "start": "node dist/server/index.js",
    "scripts": {
        "dev": "nodemon --legacy-watch --delay 2 --exec babel-node src/server/index.js",
        "build": "webpack --progress --config webpack.prod.config.js && babel src/server --out-dir dist/server && npm run copy",
        "webpack-dev": "webpack-dev-server --progress --color --hot",
        "start": "node  dist/server/index.js",
        "copy": "copy -r src/frontend/admin/assets/tinymce/js/tinymce/* dist/public/modules && copy -r src/frontend/public/assets/direct/* dist/public/modules && copy -r src/server/views dist/server/ && copy -r src/frontend/game/assets/music dist/public/modules/",
        "clean": "rm -rf dist/public/modules && rm -rf dist/server"
    },
